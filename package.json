{
  "name": "pm-info-aggregator",
  "version": "0.0.1",
  "description": "Alexa Skill for my master thesis",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "prepare-package-json": "node prepare-package-json.js",
    "deploy:lambda": "npm run compile && npm run prepare-package-json && npm --prefix ./src/lambda/dist install ./src/lambda/dist && ask deploy -t lambda",
    "deploy:skill": "ask deploy -t skill",
    "deploy:model": "ask deploy -t model",
    "deploy": "npm run deploy:skill && npm run deploy:model && npm run deploy:lambda",
    "deploy:ngrok": "node ngrok-deploy.js",
    "server:start": "npm run compile && node dist/alexa-app/app.js",
    "dev": "nodemon",
    "start": "concurrently -n \"ngrok-deploy,server\" \"npm run deploy:ngrok\" \"npm run server:start\"",
    "test": "jasmine-ts --config=test/support/jasmine.json"
  },
  "author": "Philippe Schild",
  "license": "MIT",
  "dependencies": {
    "alexa-app": "^4.2.3",
    "ask-sdk": "^2.3.0",
    "ask-sdk-v1adapter": "^2.3.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.11",
    "@types/node": "^10.12.3",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "concurrently": "^4.0.1",
    "copyfiles": "^2.1.0",
    "edit-json-file": "^1.1.0",
    "jasmine": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ngrok": "^3.1.0",
    "nodemon": "^1.18.6",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "typescript": "^3.1.6"
  }
}
